{
  "expo": {
    "name": "Diamond",
    "slug": "diamond",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "scss",
        "sass"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "package": "diamond.apk"
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1e41ea3a-e2f2-49a2-bbd1-9752cb91b6b3"
      }
    }
  }
}
